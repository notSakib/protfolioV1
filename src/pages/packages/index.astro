---
import { getCollection } from "astro:content";
import Layout from "@/layouts/BlogLayout.astro";
const blogEntries = await getCollection("packages");
const obj = {
  title: "Md Saban",
};
---
<Layout postDetails={obj}>
  {
    blogEntries.map((item) => {
      const slug = `/packages/${item.slug}`
      return (
      <a href={slug} class="block p-3 border border-stone-400 mb-4">
        <div class="text-2xl">{item.data.title}</div>
      </a>
    )})
  }
</Layout>
